<main class="settings">
    <div class="settings__to-chat">
        {{{toChatLink}}}
    </div>
    {{{avatar}}}
    <h1 class="settings__login">{{login}}</h1>
    <section class="settings__items">
        {{#each settingsItems}}
            {{{this}}}
        {{/each}}
    </section>
    <section class="settings__links">
        {{{changePersonalDataLink}}}
        {{{changePasswordLink}}}
        {{{logOutLink}}}
    </section>
    {{{changePersonalDataModal}}}
    {{{changePasswordModal}}}
</main>